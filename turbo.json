{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["tailwind:generate", "^build"],
      "outputs": ["dist/**", ".next/**", "storybook-static/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "dependsOn": ["^tailwind:generate"]
    },
    "tailwind:generate": {
      "cache": false,
      "outputs": ["dist.css"]
    },
    "storybook": {
      "cache": false,
      "dependsOn": ["^tailwind:generate"]
    }
  }
}
